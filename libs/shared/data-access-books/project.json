{
  "projectType": "library",
  "sourceRoot": "libs/shared/data-access-books/src",
  "prefix": "books",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/shared/data-access-books"],
      "options": {
        "jestConfig": "libs/shared/data-access-books/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/shared/data-access-books/**/*.ts",
          "libs/shared/data-access-books/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:shared", "type:data-access"]
}
