{
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "angular.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "npmScope": "bus-demo",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "bus-demo": {
      "tags": ["scope:bus", "type:app"]
    },
    "bus-demo-e2e": {
      "tags": [],
      "implicitDependencies": ["bus-demo"]
    },
    "without-bus-demo": {
      "tags": ["scope:without", "type:app"]
    },
    "without-bus-demo-e2e": {
      "tags": [],
      "implicitDependencies": ["without-bus-demo"]
    },
    "gyrus-ui-output-bus": {
      "tags": []
    }
  }
}
